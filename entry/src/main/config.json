{
  "app": {
    "bundleName": "cn.qixqi.pan",
    "vendor": "qixqi",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 5,
      "target": 5,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {
    "default": {
      "network": {
        "cleartextTraffic": true
      }
    }
  },
  "module": {
    "package": "cn.qixqi.pan",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.NoTitleBar",
              "extra": ""
            }
          ]
        }
      },
      {
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.AuthAbility",
        "icon": "$media:icon",
        "description": "$string:authability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.NoTitleBar",
              "extra": ""
            }
          ]
        }
      },
      {
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.FileSystemAbility",
        "icon": "$media:icon",
        "description": "$string:filesystemability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.NoTitleBar",
              "extra": ""
            }
          ]
        }
      },
      {
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.FileSharingAbility",
        "icon": "$media:icon",
        "description": "$string:filesharingability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.NoTitleBar",
              "extra": ""
            }
          ]
        }
      },
      {
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.ProfileAbility",
        "icon": "$media:icon",
        "description": "$string:profileability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.NoTitleBar",
              "extra": ""
            }
          ]
        }
      },
      {
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.FileHistoryAbility",
        "icon": "$media:icon",
        "description": "$string:filehistoryability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.NoTitleBar",
              "extra": ""
            }
          ]
        }
      },
      {
        "visible": true,
        "permissions": [
          "cn.qixqi.pan.data.DataAbilityShellProvider.PROVIDER"
        ],
        "name": "cn.qixqi.pan.data.FileDownloadDataAbility",
        "icon": "$media:icon",
        "description": "$string:filedownloaddataability_description",
        "type": "data",
        "uri": "dataability://cn.qixqi.pan.data.FileDownloadDataAbility"
      },
      {
        "visible": true,
        "permissions": [
          "cn.qixqi.pan.data.DataAbilityShellProvider.PROVIDER"
        ],
        "name": "cn.qixqi.pan.data.FileUploadDataAbility",
        "icon": "$media:icon",
        "description": "$string:fileuploaddataability_description",
        "type": "data",
        "uri": "dataability://cn.qixqi.pan.data.FileUploadDataAbility"
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.INTERNET"
      },
      {
        "mergeRule": {
          "replace": [
            "usedScene"
          ]
        },
        "name": "ohos.permission.READ_MEDIA",
        "reason": "read media",
        "usedScene": {
          "ability": [
            "com.zhihu.matisse.MatisseAbility",
            "com.zhihu.matisse.slice.AlbumPreviewAbility",
            "com.zhihu.matisse.slice.CameraAbility",
            "cn.qixqi.pan.FileSystemAbility"
          ],
          "when": "always"
        }
      },
      {
        "mergeRule": {
          "replace": [
            "usedScene"
          ]
        },
        "name": "ohos.permission.WRITE_MEDIA",
        "reason": "write media",
        "usedScene": {
          "ability": [
            "com.zhihu.matisse.MatisseAbility",
            "com.zhihu.matisse.slice.AlbumPreviewAbility",
            "com.zhihu.matisse.slice.CameraAbility",
            "cn.qixqi.pan.FileSystemAbility"
          ],
          "when": "always"
        }
      },
      {
        "mergeRule": {
          "replace": [
            "usedScene"
          ]
        },
        "name": "ohos.permission.MEDIA_LOCATION",
        "reason": "read media location",
        "usedScene": {
          "ability": [
            "com.zhihu.matisse.MatisseAbility",
            "com.zhihu.matisse.slice.AlbumPreviewAbility",
            "com.zhihu.matisse.slice.CameraAbility",
            "cn.qixqi.pan.FileSystemAbility"
          ],
          "when": "always"
        }
      },
      {
        "mergeRule": {
          "replace": [
            "usedScene"
          ]
        },
        "name": "ohos.permission.CAMERA",
        "reason": "camera",
        "usedScene": {
          "ability": [
            "com.zhihu.matisse.MatisseAbility",
            "com.zhihu.matisse.slice.AlbumPreviewAbility",
            "com.zhihu.matisse.slice.CameraAbility",
            "cn.qixqi.pan.FileSystemAbility"
          ],
          "when": "always"
        }
      },
      {
        "mergeRule": {
          "replace": [
            "usedScene"
          ]
        },
        "name": "ohos.permission.WRITE_USER_STORAGE",
        "reason": "write user storage",
        "usedScene": {
          "ability": [
            "com.zhihu.matisse.slice.CameraAbility",
            "cn.qixqi.pan.FileSystemAbility"
          ],
          "when": "always"
        }
      },
      {
        "name": "cn.qixqi.pan.data.FileDownloadDataAbility"
      },
      {
        "name": "cn.qixqi.pan.data.FileUploadDataAbility"
      }
    ],
    "defPermissions": [
      {
        "name": "cn.qixqi.pan.data.FileDownloadDataAbility",
        "grantMode": "system_grant",
        "availableScope": [
          "signature"
        ]
      },
      {
        "name": "cn.qixqi.pan.data.FileUploadDataAbility",
        "grantMode": "system_grant",
        "availableScope": [
          "signature"
        ]
      }
    ]
  }
}