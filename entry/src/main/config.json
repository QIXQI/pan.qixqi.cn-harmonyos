{
  "app": {
    "bundleName": "cn.qixqi.pan",
    "vendor": "qixqi",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 5,
      "target": 5,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {
    "default": {
      "network": {
        "cleartextTraffic": true
      }
    }
  },
  "module": {
    "package": "cn.qixqi.pan",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.AuthAbility",
        "icon": "$media:icon",
        "description": "$string:authability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.NoTitleBar",
              "extra": ""
            }
          ]
        }
      },
      {
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.FileSystemAbility",
        "icon": "$media:icon",
        "description": "$string:filesystemability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.NoTitleBar",
              "extra": ""
            }
          ]
        }
      },
      {
        "orientation": "unspecified",
        "name": "cn.qixqi.pan.FileSharingAbility",
        "icon": "$media:icon",
        "description": "$string:filesharingability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.NoTitleBar",
              "extra": ""
            }
          ]
        }
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.INTERNET"
      }
    ]
  }
}